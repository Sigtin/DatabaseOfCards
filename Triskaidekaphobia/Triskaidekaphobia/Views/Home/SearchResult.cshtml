@model TriskaidekaphobiaLib.Models.MTGCardList

@{
    ViewBag.Title = "SearchResult";
}

<h2>Results</h2>

<p>"@Model" Results Found</p>

@{
    int i = 0;


    foreach (var card in Model.Cards)
    {
        if (i < 100)
        {
           @Html.Partial("_MTGCardView", card);
        }
        else
        {
            break;
        }
        i++;
    }
}